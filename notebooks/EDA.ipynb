import pandas as pd
import plotly.express as px

# Lendo os dados
car_data = pd.read_csv('vehicles_us.csv')

# Criar um histograma
fig = px.histogram(car_data, x="odometer")
fig.show()

# Criar um gráfico de dispersão
fig2 = px.scatter(car_data, x="odometer", y="price")
fig2.show()

